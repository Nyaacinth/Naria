{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "pnpm run build-frontend",
        "beforeDevCommand": "pnpm run serve-frontend-dev --port 5173 --strictPort",
        "devPath": "http://localhost:5173",
        "distDir": "../dist"
    },
    "package": {
        "productName": "Should I do itï¼Ÿ",
        "version": "../package.json"
    },
    "tauri": {
        "allowlist": {
            "window": {
                "setFocus": true,
                "show": true,
                "startDragging": true
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "work.nyaacinth.naria",
            "longDescription": "",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "title": "Should I do it?",
                "width": 400,
                "height": 300,
                "fullscreen": false,
                "resizable": false,
                "titleBarStyle": "Overlay",
                "hiddenTitle": true,
                "visible": false
            }
        ]
    }
}
